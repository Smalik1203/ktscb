{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_NEW_ARCH_ENABLED": "1"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_NEW_ARCH_ENABLED": "1"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_NEW_ARCH_ENABLED": "1"
      }
    },

    "kts:preview": {
      "extends": "preview",
      "env": {
        "SCHOOL": "kts",
        "EXPO_PUBLIC_NEW_ARCH_ENABLED": "1"
      }
    },
    "kts:production": {
      "extends": "production",
      "env": {
        "SCHOOL": "kts",
        "EXPO_PUBLIC_NEW_ARCH_ENABLED": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
